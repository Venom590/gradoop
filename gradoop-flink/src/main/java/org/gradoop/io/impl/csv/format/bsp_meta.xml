<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<datasource>

    <csvFile name="graph.csv" charset="utf8" seperator="|" type="graph">
        <label>
            <specified>Graph</specified>
        </label>
        <key>
            <id>
                <field>0</field>
            </id>
            <sys>AllSystems</sys>
        </key>
        <columns>
            <column name="id" type="Long"/>
            <column name="nodeCount" type="Integer"/>
        </columns>
    </csvFile>

    <csvFile name="book.csv" charset="utf8" seperator="|" type="vertex">
        <label>
            <field>1</field>
            <field>2</field>
        </label>
        <containment>
            <graphid>1</graphid>
        </containment>
        <key>
            <id>
                <field>0</field>
            </id>
            <sys>MyBooks</sys>
        </key>
        <columns>
            <column name="isbn" type="String"/>
            <column name="author" type="String"/>
            <column name="title" type="String"/>
        </columns>
    </csvFile>

    <csvFile name="author.csv" charset="utf8" seperator="|" type="vertex">
        <label>
            <field>0</field>
        </label>
        <containment>
            <graphid>1</graphid>
            <graphid>2</graphid>
        </containment>
        <key>
            <id>
                <field>0</field>
                <field>3</field>
            </id>
            <sys>AllAuthors</sys>
        </key>
        <columns>
            <column name="name" type="String"/>
            <column name="age" type="String"/>
            <column name="phone" type="String"/>
            <column name="zip" type="String"/>
        </columns>
    </csvFile>

    <csvFile name="writes.csv" charset="utf8" seperator="|" type="edge">
        <label>
            <specified>writes</specified>
        </label>
        <containment>
            <graphid>1</graphid>
        </containment>
        <edgeids sourceid="1" targetid="2"/>
        <key>
            <id>
                <field>1</field>
                <field>2</field>
            </id>
            <sys>AllAuthors</sys>
        </key>
        <columns>
            <column name="date" type="String"/>
            <column name="sourceId" type="Long"/>
            <column name="targetID" type="Long"/>
            <column name="teamWork" type="Boolean"/>
        </columns>
    </csvFile>

</datasource>


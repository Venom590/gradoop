<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<datasource xmlns="http://www.gradoop.org/csvInput"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.gradoop.org/csvInput
            http://www.gradoop.org/csvInput/csv_format.xsd">

    <domain name="">

        <csv name="test_graph.csv" charset="utf8" saperator="|">
            <columns>
                <column id="1" pos="1" type="Long"/>
            </columns>
            <graphhead>
                <label separator="_">
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </label>

                <key domain_name=""> <!-- separator intern e.g. arrays -->
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </key>
            </graphhead>
        </csv>
        <csv name="test_graph.csv" charset="utf8" saperator="|">
            <columns>
                <column id="1" type="Long"/>
            </columns>
            <vertex>
                <properties>
                    <property column_id="" name=""/>
                </properties>

                <label separator="_">
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </label>

                <key domain_name="">
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </key>

                <edges>
                    <reference datasource_name="">
                        <key domain_name="">
                            <static name="graph"/> <!-- static oder ref column-->
                            <!-- <ref column_id="1"/>  <!- columnref -->
                        </key>
                        <properties>
                            <property column_id="" name=""/>
                        </properties>
                    </reference>
                </edges>
                <graphs>
                    <reference datasource_name="">
                        <key domain_name="">
                            <static name="graph"/> <!-- static oder ref column-->
                            <!-- <ref column_id="1"/>  <!- columnref -->
                        </key>
                    </reference>
                </graphs>
            </vertex>
        </csv>
        <csv name="test_graph.csv" charset="utf8" saperator="|">
            <columns>
                <column id="1" pos="1" type="Long"/>
            </columns>
            <edge>
                <label separator="_">
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </label>
                <key domain_name=""> <!-- optional-->
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </key>
                <source>
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </source>
                <target>
                    <static name="graph"/> <!-- static oder ref column-->
                    <!-- <ref column_id="1"/>  <!- columnref -->
                </target>
                <properties>
                    <property column_id="" name=""/>
                </properties>
                <graphs>
                    <reference datasource_name="">
                        <key domain_name="">
                            <static name="graph"/> <!-- static oder ref column-->
                            <!-- <ref column_id="1"/>  <!- columnref -->
                        </key>
                    </reference>
                </graphs>
            </edge>
        </csv>
    </domain>
</datasource>
